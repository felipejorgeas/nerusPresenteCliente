{
  "title":
  {
    "show": true,
    "logo": "<img src='img/logo-cliente.png' />",
    "subtitle": "BUSCA DE PRODUTOS"
  },
  "drawer":
  {
    "show": false,
    "onclick": "menuSec();"
  },
  "back":
  {
    "show": true,
    "onclick": "loadPage('', true, true);"
  },
  "actions":
  {
    "show": true,
    "itens":
    [
      {
        "onclick": "actionSearch();",
        "icon": "search",
        "type": "produto",
        "barcode": true
      },
      {
        "onclick": "prdAddList();",
        "icon": "accept"
      }
    ]
  },
  "menu":
  {
    "show": false,
    "itens":
    [
      {
        "onclick": "loadPage('lista-produto');",
        "label": "Lista atual"
      }
    ]
  },
  "nav":
  {
    "show": true,
    "type": "produto-busca"
  }
}
